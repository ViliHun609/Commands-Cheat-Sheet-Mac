{
  "name": "ffmpeg",
  "systemImageName": "film",
  "subtitle": "Video and audio manipulation",
  "items": [
    { "name": "Convert MP4 to GIF", "code": "ffmpeg -i input.mp4 output.gif" },
    { "name": "Extract Audio", "code": "ffmpeg -i input.mp4 -q:a 0 -map a output.mp3" },
    { "name": "Trim Video", "code": "ffmpeg -i input.mp4 -ss 00:00:10 -to 00:00:30 -c copy output.mp4" },
    { "name": "Resize Video", "code": "ffmpeg -i input.mp4 -vf scale=1280:720 output.mp4" },
    { "name": "Compress Video", "code": "ffmpeg -i input.mp4 -vcodec libx265 -crf 28 output.mp4" },
    { "name": "Convert to MP4", "code": "ffmpeg -i input.mov output.mp4" },
    { "name": "Combine Video + Audio", "code": "ffmpeg -i video.mp4 -i audio.mp3 -c:v copy -c:a aac output.mp4" },
    { "name": "Extract Frames", "code": "ffmpeg -i input.mp4 -vf fps=1 frame_%04d.png" },
    { "name": "Speed Up Video", "code": "ffmpeg -i input.mp4 -filter:v \"setpts=0.5*PTS\" output.mp4" },
    { "name": "Slow Down Video", "code": "ffmpeg -i input.mp4 -filter:v \"setpts=2.0*PTS\" output.mp4" },
    { "name": "Add Subtitles", "code": "ffmpeg -i input.mp4 -vf subtitles=<SUB_FILE>.srt output.mp4" },
    { "name": "Remove Audio", "code": "ffmpeg -i input.mp4 -an output.mp4" },
    { "name": "Extract Video Only", "code": "ffmpeg -i input.mp4 -an -vcodec copy output.mp4" },
    { "name": "Merge Files (same format)", "code": "ffmpeg -f concat -i list.txt -c copy output.mp4" },
    { "name": "Convert Image Sequence to Video", "code": "ffmpeg -framerate 24 -i img_%03d.png -c:v libx264 output.mp4" }
  ]
},
